unit ExercicioModel;

interface

uses RegiaoModel;

type
  TExercicio = class
  public
    FId: Integer;
    FNome: string;
    FRegiao: TRegiao;

    constructor Create(AId: Integer; const ANome: string; ARegiao: TRegiao);
  end;


implementation

constructor TExercicio.Create(AId: Integer; const ANome: string; ARegiao: TRegiao);
begin
  FId := AId;
  FNome := ANome;
  FRegiao := ARegiao;
end;

end.
