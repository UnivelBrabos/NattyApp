unit RegiaoModel;

interface

type
  TRegiao = class
  public
    FId: Integer;
    FRegiao: string;

    constructor Create(AId: Integer; const ARegiao: string);
    function SqlTodasRegioes(): string;
  end;

implementation

constructor TRegiao.Create(AId: Integer; const ARegiao: string);
begin
  FId := AId;
  FRegiao := ARegiao;
end;

end.
