unit UsuarioModel;

interface

type
  TUsuario = class
  public
    FId: Integer;
    FNome: string;
    FEmail: string;
    FSenha: string;
    FPeso: Double;

               constructor CreateUser(const nome:string;
                                      const email:string;
                                      const senha:string;
                                      Peso: double);

            end;

implementation

  constructor TUsuario.CreateUser(const nome:string;
                                  const email:string;
                                  const senha:string;
                                  Peso: double);
  begin
    UserName := nome;
    Email := email;

  end;

end.
